{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "public": "public",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "dist/main.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
